@model List<BibleVersionDatatableViewModel>

@{
    ViewData["Title"] = "Bible Version List";
}

@section VendorStyles {
    <link rel="stylesheet" type="text/css" href="~/lib/jquery/datatables/jquery.dataTables.min.css">
}

@section PageStyles {
}

@section VendorScripts {
    <script src="~/lib/jquery/datatables/jquery.dataTables.min.js"></script>
}

@section PageScripts {
    <script src="~/js/bibleview/index.js"></script>
}

<div class="container-fluid">
    <div class="social-dash-wrap">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb-main">
                    <h4 class="text-capitalize breadcrumb-title">Bible Version List</h4>
                    <div class="breadcrumb-action justify-content-center flex-wrap">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index" class="uil uil-estate"></i>Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Bible Version List</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card mt-30">
                    <div class="card-body">

                        <table id="bibledatatables" class="table">
                            <thead>
                                <tr>
                                    <th>Version</th>
                                    <th>Abbreviation</th>
                                    <th>Language</th>
                                    <th>Info URL</th>
                                    <th>Copyright</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div> 
            </div>
        </div>
    </div>
</div>
